#Conversions
#Distance
foot=int(input("enter foot"))
inches = foot * 12
print("result is  ",inches)

inches=int(input("enter inches"))
foot=inches/12
print("result is  ",foot)

yard=int(input("enter yard"))
feet=yard*3
print("result is  ",feet)

feet=int(input("enter feet"))
