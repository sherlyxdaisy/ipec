#TEMPERATURE
f=int(input("enter fahrenheit"))
celsius = (fahrenheit - 32) * 5/9
print("result is  ",celsius)

c=int(input("enter celsius"))
fahrenheit = (celsius * 9/5) + 32
print("result is  ",fahrenheit)
