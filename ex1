#TRIANGLE
num_sides=int(input("enter number of sides"))
sum_of_angles = (num_sides - 2) * 180
print("result is",sum_of_angles)

b=int(input("enter base"))
h=int(input("enter height"))
a=0.5*b*h
print("result is  ",a)


#CIRCLE
print("number of degrees in circle is 180")

d=int(input("enter diameter"))
c=d*3.14
print("result is  ",c)


r=int(input("enter radius"))
a=3.14*(r**2)
print("result is  ",a)

#CYLINDER
r=int(input("enter radius"))
h=int(input("enter height"))
p=3.14
v=p*(r**2)*h
print("result is ",v)


#CONE
r=int(input("enter radius"))
h=int(input("enter height"))
P=3.14
v=p*r*r*h/3
print("result is  ",v)


#SPHERE BALL
r=int(input("enter radius"))
p=3.14
v=4/3*p*r*r*r
print("result is  ",v)


#ELECTRICTY
